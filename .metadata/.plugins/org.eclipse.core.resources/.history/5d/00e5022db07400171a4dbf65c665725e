
public class FacultyModel {
	
	int id;
	String ssn;
	String name;
	String url;
	int votes;
	
	public FacultyModel(int id, String title, String url, int votes) {
		this.id = id;
		this.title = title;
		this.url = url;
		this.votes = votes;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getUrl() {
		return url;
	}

	public void setUrl(String url) {
		this.url = url;
	}

	public int getVotes() {
		return votes;
	}

	public void setVotes(int votes) {
		this.votes = votes;
	}
	

}
