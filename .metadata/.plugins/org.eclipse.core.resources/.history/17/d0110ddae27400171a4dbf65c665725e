
public class StudentModel {
	int id;
	String ssn;
	String name;
	int age;
	String gender;
	String deg_prog;
	
	public StudentModel(int id, String ssn, String name, int age, String gender, String deg_prog) {
		this.id = id;
		this.ssn = ssn;
		this.name = name;
		this.age = age;
		this.gender = gender;
		this.deg_prog = deg_prog;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getSsn() {
		return ssn;
	}

	public void setSsn(String ssn) {
		this.ssn = ssn;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getGender() {
		return gender;
	}

	public void setGender(String gender) {
		this.gender = gender;
	}

	public String getDeg_prog() {
		return deg_prog;
	}

	public void setDeg_prog(String deg_prog) {
		this.deg_prog = deg_prog;
	}
	
	
}
